#include <stdio.h>


int a[5][4];
int i=0 , j=0 ;
int b=0 ;
float c=0 ;


int main() {


    for(i = 0; i < 5; i++) {
        printf("%d. ogrencinin notlarini giriniz:\n", i + 1);
        for(j = 0; j < 4; j++) {
            do {
                printf("%d. not: ", j + 1);
                scanf("%d", &a[i][j]);
                if(a[i][j] < 0 || a[i][j] > 100) {
                    printf("HATA! Not 0 ile 100 arasinda olmalidir.\n");
                }
            } while(a[i][j] < 0 || a[i][j] > 100);
        }
    }

    for(i = 0; i < 5; i++) {
        b = 0;
        for(j = 0; j < 4; j++) {
            b += a[i][j];
        }
        c = b / 4.0;

        printf("%d. ogrencinin ortalamasi: %.2f -> ", i + 1, c);

        if(c < 50) {
            printf("KALDI\n");
        } else {
            printf("GECTI\n");
        }
    }

    return 0;
}
